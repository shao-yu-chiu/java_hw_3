package java_hw_3;

public class p14 {
	public static void main(String[] args) {
		Car car1;
		car1 = new Car();
				
		car1.setnum(2345);
		car1.setgas(23.67);
	}
}

class Car
{
	int num;
	double gas;
	
	void setnum(int n)
	{
		num = n;
		System.out.println("將車號設為" + num);
	}
	
	void setgas(double g)
	{
		gas = g;
		System.out.println("將汽油量設為" + gas);
	}
}
